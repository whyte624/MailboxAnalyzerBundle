{
  "name": "whyte624/mailbox-analyzer-bundle",
  "description": "Checks mailboxes, to unsubscribe (or other actions) users",
  "type": "symfony-bundle",
  "homepage": "https://github.com/whyte624/MailboxAnalyzerBundle",
  "authors": [
    {
      "name": "Andrei Sozonov",
      "email": "whyte624@gmail.com",
      "homepage": "http://whyte624.ru"
    }
  ],
  "keywords": [
    "mail",
    "imap",
    "pop3",
    "unsubscribe",
    "mailbox",
    "symfony"
  ],
  "license": [
    "MIT"
  ],
  "require": {
    "php": "~5.3 | ~7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "Whyte624\\MailboxAnalyzerBundle\\": ""
    },
    "exclude-from-classmap": [
      "Tests/"
    ]
  },
  "config": {
    "bin-dir": "bin/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}